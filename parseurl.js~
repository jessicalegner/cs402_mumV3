var qs = require('querystring');
var url = require('url');

function parseURL(urltoparse){
var query = url.parse(urltoparse).query;
	if (query != ''){
	var arguments = qs.parse(query);
	console.log(arguments);
		return arguments;
	}else return {};
	
}

parseURL('http://localhost?login=sebastien&password=toto');	
